<MudNavMenu>
    <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Rounded.Home">@Localizer["Home"]</MudNavLink>
    <MudDivider />
    <AuthorizeView Roles="Superadmin">
        <Authorized>
            <MudExpansionPanels MultiExpansion="true">
                <MudExpansionPanel Text=@Localizer["AdministratorMenu"]>
                    <MudNavLink Href="/cities" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Public">@Localizer["Cities"]</MudNavLink>
                    <MudDivider />
                    <MudNavLink Href="/hangars" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.LocationCity">@Localizer["Hangars"]</MudNavLink>
                    <MudDivider />
                    <MudNavLink Href="/users" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.People">@Localizer["Users"]</MudNavLink>
                    <MudDivider />
                </MudExpansionPanel>
            </MudExpansionPanels>
        </Authorized>
    </AuthorizeView>
    <MudDivider />
    <MudExpansionPanels MultiExpansion="true">
        <MudExpansionPanel Text=@Localizer["ITcourse"]>
            <MudDivider />
            <MudNavLink Href="/ITCourse" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.LaptopChromebook">@Localizer["Curso IT"]</MudNavLink>
        </MudExpansionPanel>
    </MudExpansionPanels>
    <MudDivider />
    <MudNavLink Href="/about" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Info">@Localizer["About"]</MudNavLink>
</MudNavMenu>